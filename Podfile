platform :ios, '8.0'

target 'Alfred' do
  use_frameworks!

  # Networking
  pod 'Alamofire'								# https://github.com/Alamofire/Alamofire
    
  # Mapping
  pod 'AlamofireObjectMapper'					# https://github.com/tristanhimmelman/AlamofireObjectMapper

  # UI
  pod 'SDWebImage'                              # https://github.com/rs/SDWebImage

end
